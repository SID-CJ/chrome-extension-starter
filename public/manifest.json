{
  "manifest_version": 3,
  "name": "Ambiloops",
  "short_name": "Extension Starter",
  "version": "1.0.0",
  "description": "Make every new tab a refreshing space that helps you relax, stay focused, and feel energized.",
  "key": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Ambiloops",
    "default_icon": {
      "16": "/icons/web/16.png",
      "48": "/icons/web/48.png",
      "128": "/icons/web/128.png"
    }
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "icons": {
    "48": "/icons/web/48.png",
    "128": "/icons/web/128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
