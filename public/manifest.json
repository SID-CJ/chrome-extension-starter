{
  "manifest_version": 3,
  "name": "Extension Starter Template",
  "short_name": "Extension Starter",
  "version": "1.0.0",
  "description": " A Chrome Extension Starter Template",
  "key": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Chrome Extension Starter",
    "default_icon": {
      "16": "icons/web/favicon.ico",
      "192": "icons/web/icon-192.png",
      "512": "icons/web/icon-512.png"
    }
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "icons": {
    "192": "icons/web/icon-192.png",
    "512": "icons/web/icon-512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
